{
  "name": "tezrun-server",
  "version": "1.0.0",
  "description": "tezrun-server",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon ./src/app.ts",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/*.ts'",
    "heroku": "env NODE_PATH=dist node dist/app.js",
    "start": "env NODE_PATH=dist pm2 start dist/app.js",
    "tslint": "tslint --project tsconfig.json",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\""
  },
  "engines": {
    "node": "16.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reactomoss/tez-run-server.git"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "crypto-js": "^4.0.0",
    "date-diff": "^0.2.2",
    "dotenv": "^10.0.0",
    "ejs": "3.1.6",
    "errorhandler": "1.5.1",
    "express": "4.17.1",
    "express-jwt": "5.3.3",
    "express-session": "1.17.2",
    "express-useragent": "^1.0.15",
    "express-validator": "^6.12.0",
    "jsonwebtoken": "8.5.1",
    "method-override": "3.0.0",
    "methods": "1.1.2",
    "moment": "^2.29.1",
    "mongoose": "5.12.14",
    "mongoose-unique-validator": "2.0.3",
    "morgan": "1.10.0",
    "randomstring": "^1.2.1",
    "serve-favicon": "^2.5.0",
    "slug": "5.1.0",
    "timezones-list": "^3.0.1",
    "underscore": "1.13.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.0.2",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.2",
    "@types/express-session": "^1.17.4",
    "@types/express-useragent": "^1.0.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/method-override": "^0.0.32",
    "@types/mocha": "^9.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/mongoose-unique-validator": "^1.0.5",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.6.1",
    "@types/randomstring": "^1.1.7",
    "@types/serve-favicon": "^2.5.3",
    "@types/uuid": "^8.3.1",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "mocha": "^9.1.2",
    "newman": "^5.2.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.0",
    "tsconfig-paths": "^3.10.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5"
  }
}
